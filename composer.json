{
  "name": "jdwx/web",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "JDWX\\Web\\": "src/",
      "JDWX\\Web\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "jetbrains/phpstorm-attributes": "^1.0",
    "phpunit/phpunit": "^12",
    "phan/phan": "^v5",
    "phpstan/phpstan": "^2.0"
  },
  "require": {
    "php": ">=8.3.0",
    "jdwx/json-php": "^1.0",
    "jdwx/log": "^1.0",
    "jdwx/param": "^1.0",
    "jdwx/strict": "^1.0",
    "jdwx/stream": "^1.0",
    "jdwx/trie": "^1.0",
    "psr/log": "^3.0"
  },
  "scripts": {
    "phan": "PHAN_ALLOW_XDEBUG=1 php -d xdebug.mode=off vendor/bin/phan --no-progress-bar -S --analyze-twice",
    "phpstan": "php -d xdebug.so vendor/bin/phpstan --no-progress",
    "test": "vendor/bin/phpunit tests"
  }
}
